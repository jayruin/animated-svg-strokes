{
  "type": "module",
  "devDependencies": {
    "@stylistic/eslint-plugin": "1.5.3",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "eslint": "8.56.0",
    "parcel": "2.11.0",
    "typescript": "5.3.3"
  },
  "scripts": {
    "types": "tsc --noEmit",
    "lint": "eslint src",
    "style": "eslint src --no-eslintrc --config .eslintrc.styles.json",
    "format": "eslint src --no-eslintrc --config .eslintrc.styles.json --fix",
    "build": "parcel build",
    "reset": "git clean -fdx",
    "update": "node scripts/rm.js node_modules pnpm-lock.yaml && pnpm update --latest"
  },
  "targets": {
    "strokes": {
      "context": "browser",
      "distDir": "dist",
      "isLibrary": true,
      "optimize": true,
      "outputFormat": "esmodule",
      "source": "src/strokes.ts",
      "sourceMap": false
    }
  }
}
